<?xml version="1.0" encoding="utf-8"?>
<Filter>
	<Name>GBHV</Name>
	<MessageFilters>
		<Filter>
			<Enable>true</Enable>
			<Text>[*]</Text>
			<MatchType>Simple</MatchType>
			<FilterType>Include</FilterType>
			<BackColor>
				<Red>0</Red>
				<Green>0</Green>
				<Blue>0</Blue>
			</BackColor>
			<TextColor>
				<Red>128</Red>
				<Green>255</Green>
				<Blue>255</Blue>
			</TextColor>
		</Filter>
		<Filter>
			<Enable>true</Enable>
			<Text>[!]</Text>
			<MatchType>Simple</MatchType>
			<FilterType>Include</FilterType>
			<BackColor>
				<Red>0</Red>
				<Green>0</Green>
				<Blue>0</Blue>
			</BackColor>
			<TextColor>
				<Red>255</Red>
				<Green>0</Green>
				<Blue>0</Blue>
			</TextColor>
		</Filter>
		<Filter>
			<Enable>true</Enable>
			<Text>-------------------------------------------------</Text>
			<MatchType>Simple</MatchType>
			<FilterType>Clear</FilterType>
			<BackColor>
				<Red>255</Red>
				<Green>255</Green>
				<Blue>255</Blue>
			</BackColor>
			<TextColor>
				<Red>0</Red>
				<Green>0</Green>
				<Blue>0</Blue>
			</TextColor>
		</Filter>
		<Filter>
			<Enable>true</Enable>
			<Text>[+] </Text>
			<MatchType>Simple</MatchType>
			<FilterType>Include</FilterType>
			<BackColor>
				<Red>0</Red>
				<Green>0</Green>
				<Blue>0</Blue>
			</BackColor>
			<TextColor>
				<Red>0</Red>
				<Green>255</Green>
				<Blue>0</Blue>
			</TextColor>
		</Filter>
		<Filter>
			<Enable>true</Enable>
			<Text>[DEBUG] </Text>
			<MatchType>Simple</MatchType>
			<FilterType>Include</FilterType>
			<BackColor>
				<Red>0</Red>
				<Green>0</Green>
				<Blue>0</Blue>
			</BackColor>
			<TextColor>
				<Red>192</Red>
				<Green>192</Green>
				<Blue>192</Blue>
			</TextColor>
		</Filter>
	</MessageFilters>
	<ProcessFilters/>
</Filter>
